                                    <i class="<%# Eval("Icon") %>"></i><span><%# Eval("Formname") %></span> <i class="bi bi-chevron-down ms-auto"></i>
                                </a>
                                <ul id="#ul<%# Eval("Formid") %>" class="nav-content collapse" data-bs-parent="#sidebar-nav" style="">
                                    <asp:Repeater ID="rpt_child" runat="server">
                                        <ItemTemplate>
                                            <li>
                                                <a href="<%# Eval("pageurl") %>"><i class="bi bi-circle"></i><span><%# Eval("Formname") %></span></a>
                                            </li>
                                        </ItemTemplate>
                                    </asp:Repeater>
                                </ul>
                            </li>
                        </ItemTemplate>
                    </asp:Repeater>
                </ul>

