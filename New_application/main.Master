            <scril/pt src="<%= ResolveUrl("~/assets/js/pcoded.min.js")%>"></scril/pt>
            <script>
                debugger;
                $('document').ready(function () {
                    debugger;
                    $('li.nav-item a').click(function (e) {
                        var target = e.target.dataset.bsTarget
                        if (document.getElementById(target).classList[2] == 'show') {
                            document.getElementById(target).classList.remove('show')
                        }
                        else {
                            document.getElementById(target).classList.add
                                ('show')
                        }
                    });
                })
            </script>

